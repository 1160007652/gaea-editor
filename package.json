  "types": "src/index.ts",
  "run-react": {
    "entrys": [
      "src/develop-entry/index"
    ],
    "rules": [
      {
        "test": ".css$",
        "use": [
          "style-loader",
          "css-loader"
        ]
      }
    ],
    "production": {
      "path": "built",
      "filename": "bundle.js"
    }
  },
