{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "target": "es6",
    "removeComments": true,
    "preserveConstEnums": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "built",
    "skipLibCheck": true,
    "sourceRoot": "src",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": ["node_modules", "built", "built-production"]
}
